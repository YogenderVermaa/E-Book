    "eslint-plugin-react-hooks": "^7.0.1",
    "husky": "^8.0.0",
    "install": "^0.13.0",
    "lint-staged": "^16.2.7",
    "npm": "^11.7.0",
    "prettier": "^3.7.4",
    "prettier-plugin-tailwindcss": "^0.7.2"
  },
  "dependencies": {
    "react-hot-toast": "^2.6.0"
